# 服务器设置
PORT=4000
NODE_ENV=development
ALLOWED_ORIGINS=http://localhost:5173,https://your-prod-url.com

# 管理员设置
ADMIN_KEY=your-admin-key

# AI API设置
VITE_API_KEY=your-api-key
VITE_API_URL=https://dashscope.aliyuncs.com/compatible-mode/v1
